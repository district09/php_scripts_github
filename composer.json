{
    "name": "digipolisgent/github-scripts",
    "homepage": "https://github.com/digipolisgent/php_scripts_github",
    "description": "DigipolisGent Github",
    "keywords": [
        "DigipolisGent",
        "github"
    ],
    "type": "project",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Peter Decuyper",
            "email": "peter.decuyper@DigipolisGent.gent"
        },
        {
            "name": "Dieter Blomme",
            "email": "dieterblomme@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": "^7.0.1",
        "http-interop/http-factory-guzzle": "^1.0",
        "knplabs/github-api": "^3.0",
        "phpunit/phpunit": "^9.0",
        "psr/log": "^1.0",
        "symfony/cache": "^5.3",
        "symfony/console": "^5.0",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "DigipolisGent\\Github\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigipolisGent\\Tests\\Github\\": "tests/"
        }
    },
    "bin": [
        "bin/github"
    ],
    "config": {
        "sort-packages": true,
        "bin-dir": "bin/",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "require-dev": {
        "phpmd/phpmd": "^2.6",
        "squizlabs/php_codesniffer": "^3.2"
    }
}
